{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Delay": "5000",
  "RenumerateAt": "03:00",
  "Consolidate": [
    {
      "Mask": "*.*",
      "Source": "d:\\temp\\source",
      "Target": "d:\\temp\\target"
    },
    {
      "Mask": "*.*",
      "Source": "d:\\temp\\source1",
      "Target": "d:\\temp\\target1"
    }
  ]
}
